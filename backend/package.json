{
  "name": "game-server",
  "version": "1.0.0",
  "description": "backend service for tic-tac-toe",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon dist/index.js -e ts\"",
    "start": "node dist/index.js",
    "build": "tsc -p src",
    "build:docker": "docker build -t audiph/tic-tac-toe-server .",
    "docker:compose": "docker-compose up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.3.0"
  }
}
